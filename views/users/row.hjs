<tr class="align-middle">
    <td>
        <span class="btn btn-danger" hx-delete="/users/{{userID}}" hx-target="closest tr">
            <i class="bi bi-trash3-fill"></i>
        </span>
        {{userID}}
    </td>
    <td>
        {{firstName}} {{lastName}}
    </td>
    <td>
        {{email}}
    </td>
    <td hx-get="/users/{{userID}}" hx-target="#main">
        <i class="bi bi-arrow-right btn btn-info"></i>
    </td>
</tr>